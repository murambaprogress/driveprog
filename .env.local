# DriveCash Dashboard Environment Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api

# Authentication
REACT_APP_AUTH_TOKEN_KEY=accessToken
REACT_APP_REFRESH_TOKEN_KEY=refreshToken

# Real-time Features
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_POLLING_INTERVAL=30000

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Feature Flags
REACT_APP_ENABLE_API_FALLBACK=true
REACT_APP_ENABLE_STATIC_FALLBACK=true
REACT_APP_ENABLE_PROFILE_POLLING=true